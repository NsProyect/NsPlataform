<div class="aptoHome">
  <table class="table table-striped table-hover ">
    <thead>
      <tr>
        <th></th>
      </tr>
    </thead>

    <tbody>
    <div class="busqueda">
      <%= form_tag root_path, name: 'search', method: 'get', class: "form-inline", role: "form" do %>
          <label>*Costo($)</label>
          <div class="form-group">
            <%= number_field_tag :costo, nil, required: true %>
          </div>
          <label>*Habitaciones(#)</label>
          <div class="form-group">
            <%= number_field_tag :room, nil,required: true %>
          </div>
          <label>*Fecha Inicio</label>
          <div class="form-group">
            <%= text_field_tag :datein, nil,class: 'datepicker', required: true %>
          </div>
          <label>*Fecha Hasta</label>
          <div class="form-group">
            <%= text_field_tag :dateout,nil, class: 'datepicker', required: true %>
          </div>
          <div class="form-group">
            <button class="btn btn-success btn-sm">
                <%= image_submit_tag("Buscar") %>
            </button>
          </div>
      <% end %>
    </div>
      <% @apartments.each do |apartment| %>
          <td><div class="img"><%= image_tag apartment.image.url(:thumb) %><div class="form"><div class="des"><%= apartment.amenities %></div><br><%= link_to "ver", apartment_path(apartment), :class => "btn btn-default btn-xs"%></div></td>
      <% end %>
    </tbody>
  </table>
</div>
</div>